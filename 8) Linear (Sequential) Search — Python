# file: linear_search.py
def linear_search(arr, target):
    for i, val in enumerate(arr):
        if val == target:
            return i
    return -1

def main():
    array = [3, 5, 2, 7, 9, 1, 4]
    target = 7
    res = linear_search(array, target)
    if res != -1:
        print(f"Элемент найден на позиции: {res}")
    else:
        print("Элемент не найден")

if __name__ == "__main__":
    main()
